<?xml version="1.0" encoding="UTF-8"?>
<rule-execution-set>
   <name>RuleExecutionSet1</name>
   <description>Rule Execution Set</description>

   <synonymn name="prop" class="org.jruleengine.Clause" />

    <!--
      if Socrate is human then Socrate is mortal
    -->
   <rule name="Rule2" description="if Socrate is human then Socrate is mortal" >
	<if leftTerm="Socrate is human" />
	<then method="prop.setClause" arg1="Socrate is mortal" />
   </rule>

</rule-execution-set>
